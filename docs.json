{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Elata Docs",
  "colors": {
    "primary": "#7D66D9",
    "light": "#7D66D9",
    "dark": "#607274"
  },
  "favicon": "/favicon.svg",
  "background": {
    "decoration": "gradient",
    "color": {
      "light": "#F8F5EE",
      "dark": "#171717"
    }
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Explore",
          "icon": "map",
          "color": {
            "light": "#7D66D9",
            "dark": "#7D66D9"
          },
          "href": "https://app.elata.bio"
        },
        {
          "anchor": "Blog",
          "icon": "newspaper",
          "color": {
            "light": "#7D66D9",
            "dark": "#7D66D9"
          },
          "href": "https://github.com/elata-biosciences"
        },
        {
          "anchor": "Discord Community",
          "icon": "discord",
          "color": {
            "light": "#5865F2",
            "dark": "#5865F2"
          },
          "href": "https://discord.com/invite/GqS9CstffK"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Learn",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "learn/protocol-overview",
              "learn/how-it-works"
            ]
          },
          {
            "group": "Economics",
            "pages": [
              "learn/tokenomics",
              "learn/revenue-model"
            ]
          },
          {
            "group": "Governance",
            "pages": [
              "home/governance/structure",
              "home/governance/teams"
            ]
          }
        ]
      },
      {
        "tab": "Builders Reference",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "builders/getting-started",
              "builders/launch-your-app"
            ]
          },
          {
            "group": "App Economics",
            "pages": [
              "builders/bonding-curve-economics",
              "builders/grow-your-community"
            ]
          },
          {
            "group": "Integration",
            "pages": [
              "builders/api-overview"
            ]
          }
        ]
      },
      {
        "tab": "User Hub",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "users/explore-apps",
              "users/buy-and-trade"
            ]
          },
          {
            "group": "Earning & Governance",
            "pages": [
              "users/stake-and-earn",
              "users/veelta-governance"
            ]
          }
        ]
      },
      {
        "tab": "Resources",
        "groups": [
          {
            "group": "Reference",
            "pages": [
              "resources/smart-contracts",
              "resources/faq"
            ]
          },
          {
            "group": "Community",
            "pages": [
              "resources/media-assets",
              "resources/references"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/images/docs-light-mode.svg",
    "dark": "/images/docsdarkmode.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Apps Ecosystem",
        "href": "https://app.elata.bio"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Main Site",
      "href": "https://elata.bio/"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/elata_bio",
      "github": "https://github.com/elata-biosciences",
      "linkedin": "https://linkedin.com/company/elata-biosciences"
    }
  },
  "fonts": {
    "family": "Inter"
  },
  "styling": {
    "latex": true
  },
  "icons": {
    "library": "lucide"
  }
}
