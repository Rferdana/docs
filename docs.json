{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Elata Docs",
  "colors": {
    "primary": "#FF797B",
    "light": "#F8F5EE",
    "dark": "#607274"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Learn",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Info Hub",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://elata.mintlify.app/",
          "icon": ""
        },
        {
          "anchor": "Blog",
          "href": "https://www.elata.bio/blog",
          "icon": ""
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Explore Ecosystem",
        "href": "https://www.elata.bio/ecosystem"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Apps Dashboard",
      "href": "https://www.elata.bio/"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/elata_bio",
      "github": "https://github.com/elata-biosciences",
      "linkedin": "https://linkedin.com/company/elata-biosciences"
    }
  }
}
